{
  "expo": {
    "name": "Jagrut",
    "slug": "jagrut",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jagrut.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jagrut.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "metaData": {
        "com.google.mlkit.vision.DEPENDENCIES": "ocr,barcode"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Jagrut to access your camera to capture bills."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Jagrut to access your photos to select bills."
        }
      ],
      "onnxruntime-react-native",
      "./plugins/withOnnxManualLinking"
    ],
    "extra": {
      "eas": {
        "projectId": "e94c04a0-f32e-4583-9f96-5eb766143d60"
      }
    }
  }
}
